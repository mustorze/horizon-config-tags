{
  "name": "mustorze/horizon-config-tags",
  "description": "A monitored tag standardizer for Laravel Horizon",
  "type": "library",
  "require": {
    "php": ">=7.1",
    "laravel/horizon": "^2.0|^3.0",
    "laravel/framework": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Henrique Duarte Coelho",
      "email": "mustorze@mustorze.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Mustorze\\ConfigTags\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mustorze\\ConfigTags\\HorizonConfigTagsProvider"
      ]
    }
  }
}
